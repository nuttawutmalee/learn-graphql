{
  "minified": true,
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }],
    "flow"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-strip-types"
  ],
  "env": {
    "production": {
      "presets": [
        [
          "minify",
          {
            "mangle": false,
            "evaluate": false
          }
        ]
      ]
    }
  }
}