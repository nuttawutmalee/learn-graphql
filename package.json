    "mkdirp": "^0.5.1",
    "mongoose": "^5.1.4",
    "nodemailer": "^4.6.6",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pretty-error": "^2.1.1",
    "semver": "^5.5.0",
    "shortid": "^2.2.8",
    "strong-error-handler": "^3.0.0"
  }
