{
  "name": "learn",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "start": "nodemon --exec babel-node ./src/index.js",
    "build": "npm run clean:dist && npm run build:babel && npm run build:flow",
    "build:babel": "babel src --out-dir dist --ignore spec.js,test.js,types.js",
    "build:flow": "flow-copy-source --ignore spec.js,test.js src dist",
    "clean:dist": "rimraf dist",
    "lint": "eslint .",
    "requirements-check": "node check-version.js",
    "postinstall": "npm run requirements-check",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-typed-install": "flow-typed install -s -o",
    "flow-typed-createstub": "flow-typed create-stub -o"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-minify": "^0.4.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.74.0",
    "flow-copy-source": "^2.0.0",
    "flow-typed": "^2.4.0",
    "nodemon": "^1.17.5",
    "prettier": "1.13.4",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "apollo-upload-server": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "csurf": "^1.9.0",
    "dataloader": "^1.4.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^14.0.0-rc.1",
    "graphql-iso-date": "^3.5.0",
    "graphql-relay": "^0.5.5",
